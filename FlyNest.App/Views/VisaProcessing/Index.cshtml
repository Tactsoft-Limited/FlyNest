@model IEnumerable<FlyNest.Application.ViewModels.VmEntities.VmVisaRequirement>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="bredcumb-section visa-bredcumb">
    <div class="overlay"></div>
    <div class="bredcumb-text position-absolute h-100 w-100 d-flex justify-content-center align-items-center">
        <div class="text-center">
            <div class="page-title">
                <h3 class="fw-bold">Visa Processing</h3>
            </div>
            <div class="mt-10">
                <a asp-action="Index" asp-controller="Home">Home</a>
                <span><i class="bi bi-chevron-right mx-1" style="color: var(--primary-color);"></i></span>
                <span>Visa Processing </span>
            </div>
        </div>
    </div>
</div>

<div class="visa-processing">
    <div class="row">
        @foreach (var item in Model)
        {

            <div class="col-lg-3 col-md-3 col-sm-12">
                <div class="my-3">
                    <div class="offer-card">
                        <a href="#">
                            <img src="~/images/visa-processing/philiphines.jpg" class="img-fluid" alt="">
                        </a>
                        <div class="overlay">
                            <div class="content">
                                <h5 class="fw-bold">Philiphines</h5>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        }

    </div>
</div>

