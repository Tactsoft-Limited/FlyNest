
@{
    ViewData["Title"] = "View";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="card w-75 m-4 p-3 mx-auto">
    <div style="border-radius: 100%; height: 150px; width: 150px; background: #F8FAF5; margin: 0 auto;">
        <i style="margin: 20% 25%" class="fas fa-check fa-6x text-success"></i>
    </div>
    <h3 class="text-center">Success</h3>
    <p class="text-center">
        We received your payment;<br />
        Your transactionId is: @ViewBag.TranId <br />
        Payment amount is: @ViewBag.Amount <br />
        Payment Date : @ViewBag.TranDate
    </p>
</div>
